<div class="test">
	<div class="buttons">
		<button class="pure-button jAddtest">&plus;</button><br/>
		<button class="pure-button button-secondary jUptest">&bigtriangleup;</button><br/>
		<button class="pure-button button-secondary jDowntest">&bigtriangledown;</button><br/>
		<button class="pure-button button-error jRemovetest">&cross;</button>
	</div>
	<div>
		<label for="conditions{{test}}">Condi&ccedil;&otilde;es:</label>
		<br>
		<textarea class="conditions" id="conditions{{test}}" cols="30" rows="10">{{conditions}}</textarea>
	</div>
	<div>
		<label for="procedures{{test}}">Passos:</label>
		<br>
		<textarea class="procedures" id="procedures{{test}}" cols="30" rows="10">{{procedures}}</textarea>
	</div>
	<div>
		<label for="expected{{test}}">Resultado Experado:</label>
		<br>
		<textarea class="expected" id="expected{{test}}" cols="30" rows="10">{{expected}}</textarea>
	</div>
</div>